@model ViennaRuns.Run

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Run</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Distance
        </dt>

        <dd>
            @Html.DisplayFor(model => model.r_distance)
        </dd>

        <dt>
            Duration
        </dt>

        <dd>
            @Html.DisplayFor(model => model.r_duration)
        </dd>

        <dt>
            Date
        </dt>

        <dd>
            @Html.DisplayFor(model => model.r_date)
        </dd>

        <dt>
            Feeling after run
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FeelingAfterRun.far_datfeel)
        </dd>

        <dt>
            User
        </dt>

        <dd>
            @Html.DisplayFor(model => model.User.u_username)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.r_id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
